
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {

    // WARNING: These rules are insecure and are for development purposes only.
    // They allow any user to read and write to any part of your database.
    // You should secure your data before deploying to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
